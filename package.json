{
  "name": "exorsus.css",
  "version": "1.0.1",
  "description": "A simple and responsive flexbox based LESS Framework",
  "config": {
    "path_dist": "./dist"
  },
  "scripts": {
    "clean": "rimraf $npm_package_config_path_dist/*",
    "watch": "webpack --mode development --watch --progress --config ./config/webpack.development.js",
    "build": "export NODE_ENV=production && webpack --mode production --display-error-details --progress --config ./config/webpack.production.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IDT-media/exorsus.css.git"
  },
  "keywords": [
    "css",
    "framework",
    "flexbox",
    "less"
  ],
  "author": "Goran Ilic <ja@ichmachdas.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IDT-media/exorsus.css/issues"
  },
  "homepage": "https://github.com/IDT-media/exorsus.css#readme",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.2.0",
    "less": "^3.10.1",
    "less-loader": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
